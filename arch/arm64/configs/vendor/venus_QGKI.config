# Camera
CONFIG_USE_COMMON_CAMERA=y

# Fingerprint
CONFIG_FINGERPRINT_GOODIX_FOD=y

# Haptics
# CONFIG_INPUT_QCOM_HV_HAPTICS is not set
CONFIG_INPUT_AW8697_HAPTIC=y

# Platform
CONFIG_MACH_XIAOMI=y
CONFIG_MACH_XIAOMI_VENUS=y

# Power Supply
CONFIG_BQ_FUEL_GAUGE=y

# Touchscreen
CONFIG_TOUCHSCREEN_ST_FTS_V521_SPI=y
CONFIG_TOUCHSCREEN_XIAOMI_TOUCHFEATURE=y

# WiFi
CONFIG_CNSS_QCA6490=y

# Wireless charging
CONFIG_MI_WIRELESS=y
